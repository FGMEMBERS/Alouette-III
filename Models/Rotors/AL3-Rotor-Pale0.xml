<!--SELECT========================-->
        <animation>
		<condition>
                    <and>
			<less-than>
				<property>rotors/main/rpm</property>
				<value>150</value>
			</less-than>
                        <!--<less-than>
                                <property>surface-positions/blade-fold-pos-norm</property>
                                <value>0.50</value>
                        </less-than>-->
                    </and>
		</condition>
		<type>select</type>
                <object-name>Rotor-Pale</object-name>
 	</animation>

        <animation>
		<condition>
                    <or>
                            <and>
                                <less-than>
                                    <property>rotors/main/rpm</property>
                                    <value>210</value>
                                 </less-than>
                                 <greater-than>
                                        <property>rotors/main/rpm</property>
                                        <value>149</value>
                                 </greater-than>
                             </and>
                             <!--<not>
                                 <less-than>
                                    <property>surface-positions/blade-fold-pos-norm</property>
                                    <value>0.50</value>
                                 </less-than>
                            </not>-->
                     </or>
		</condition>
		<type>select</type>
                <object-name>Rotor-PaleD</object-name>
 	</animation>

        <animation>
		<condition>
			<greater-than>
				<property>rotors/main/rpm</property>
				<value>200</value>
			</greater-than>
		</condition>
		<type>select</type>
                <object-name>Rotor-Disc_M-Pale</object-name>
 	</animation>













