<?xml version="1.0" encoding="UTF-8"?><PropertyList><!-- ==                            == --><!-- == ROTOR PARTICLE DRY SYSTEMS == --><!-- ==             LOW            == -->	<!-- Low GROUND rotor DIRT - particle system controled by engines--><!-- effect manager supplies environment properties -->	<particlesystem>		<name>dust_generic</name>		<texture>dust_generic.png</texture>		<emissive>false</emissive>		<lighting>false</lighting>		<offsets>			<x-m> 0.0</x-m>			<y-m> 0.0</y-m>			<z-m> 0.0</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>			<property>/fdm/jsbsim/animation/rotor-particles</property>			<not>				<property>sim/paused</property>			</not>			<not>				<property>sim/crashed</property>			</not>		</condition>		<shooter>			<theta-min-deg>0</theta-min-deg>			<theta-max-deg>360</theta-max-deg>			<phi-min-deg>-0.5</phi-min-deg>			<phi-max-deg>3.2</phi-max-deg>			<speed-mps>								<property>/fdm/jsbsim/propulsion/engine/torque-lbsft</property>				<!--<property>sim/multiplay/generic/float[3]</property>-->				<!--<property>/test/torque</property>-->				<factor>1</factor>				<interpolation>					<entry><ind>    0.0 </ind><dep> 10.00 </dep></entry>					<entry><ind>  100.0 </ind><dep> 20.00 </dep></entry>					<entry><ind> 4000.0 </ind><dep> 50.00 </dep></entry>				</interpolation>				<spread>50</spread>			</speed-mps>			<rotation-speed>				<x-max-deg-sec>5</x-max-deg-sec>				<x-min-deg-sec>-5</x-min-deg-sec>				<y-max-deg-sec>5</y-max-deg-sec>				<y-min-deg-sec>-5</y-min-deg-sec>				<z-max-deg-sec>360</z-max-deg-sec>				<z-min-deg-sec>-360</z-min-deg-sec>			</rotation-speed>		</shooter>		<counter>			<particles-per-sec>								<!--<property>position/gear-agl-m</property>-->				<property>sim/multiplay/generic/float[4]</property>				<!--<property>/test/gear</property>-->				<interpolation>					<entry><ind>  0.0 </ind><dep> 115.00 </dep></entry>					<entry><ind> 10.0 </ind><dep> 15.00 </dep></entry>					<entry><ind> 20.0 </ind><dep> 0.00 </dep></entry>				</interpolation>			</particles-per-sec>		</counter>		<align>billboard</align>		<particle>			<start>				<color>					<red>						<property>/fdm/jsbsim/effects/dust-r</property>					</red>					<green>						<property>/fdm/jsbsim/effects/dust-g</property>					</green>					<blue>						<property>/fdm/jsbsim/effects/dust-b</property>					</blue>					<alpha>						<property>/fdm/jsbsim/propulsion/engine/rotor-rpm</property>						<factor>0.00004</factor>						<max>0.02</max>					</alpha>				</color>				<size>					<value>0.4</value>				</size>			</start>			<end>				<color>					<red>						<property>/fdm/jsbsim/effects/dust-r</property>					</red>					<green>						<property>/fdm/jsbsim/effects/dust-g</property>					</green>					<blue>						<property>/fdm/jsbsim/effects/dust-b</property>					</blue>					<alpha>						<property>/fdm/jsbsim/propulsion/engine/rotor-rpm</property>						<factor>0.0002</factor>						<max>0.01</max>					</alpha>				</color>								<size>					<!-- amount of Dust / end size of particle controlled by torque -->										<!--<property>/fdm/jsbsim/propulsion/engine/torque-lbsft</property>-->										<!--<property>sim/multiplay/generic/float[3]</property>-->				<!--<property>/test/torque</property>-->					<!--<factor>1</factor>					<interpolation>						<entry><ind>    0.0 </ind><dep> 0.8 </dep></entry>						<entry><ind>  400.0 </ind><dep> 1.8 </dep></entry>						<entry><ind> 3000.0 </ind><dep> 3.1 </dep></entry>						<entry><ind> 6000.0 </ind><dep> 5.2 </dep></entry>						<entry><ind> 8000.0 </ind><dep> 7.0 </dep></entry>					</interpolation>-->					<property>/fdm/jsbsim/effects/particle-size</property>					<spread>0.4</spread>				</size>			</end>			<life-sec>				<!--				<property>rotors/main/torque</property>				-->				<!--<property>sim/multiplay/generic/float[3]</property>-->				<!--<property>/test/torque</property>-->				<property>/fdm/jsbsim/propulsion/engine/torque-lbsft</property>				<factor>1</factor>				<interpolation>					<entry><ind>    0.0 </ind><dep> 0.60 </dep></entry>					<entry><ind>  400.0 </ind><dep> 1.20 </dep></entry>					<entry><ind> 3000.0 </ind><dep> 2.30 </dep></entry>					<entry><ind> 6000.0 </ind><dep> 3.00 </dep></entry>				</interpolation>			</life-sec>			<mass-kg>0.035</mass-kg>			<radius-m>0.045</radius-m>		</particle>		<program>			<fluid>air</fluid>			<gravity>false</gravity>			<wind>true</wind>		</program>	</particlesystem></PropertyList>