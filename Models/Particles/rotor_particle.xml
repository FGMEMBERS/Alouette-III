<?xml version="1.0" encoding="UTF-8"?><PropertyList><!-- ==                                   == --><!-- == ROTOR PARTICLE SYSTEMS ANIMATIONS == --><!-- ==                                   == -->		<model>		<name>dirt</name>		<path>Aircraft/Alouette-III/Models/Particles/rotor_particle_generic.xml</path>		<offsets>			<x-m>  2.973 </x-m>			<y-m>  0.000 </y-m>			<z-m> -1.900 </z-m>		</offsets>	</model>			<!-- move particle system with pich and roll -->	<animation>		<type>rotate</type>		<object-name>dirt</object-name>		 <!--<property>orientation/pitch-deg</property>  -->		<factor>1.0</factor>		<offset-deg>-2.5</offset-deg>		<property>sim/multiplay/generic/float[2]</property>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>rotate</type>		<object-name>dirt</object-name>		<!--<property>orientation/roll-deg</property> -->		<property>sim/multiplay/generic/float[3]</property>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>translate</type>		<object-name>dirt</object-name>						<!--<property>position/gear-agl-m</property>-->			<property>sim/multiplay/generic/float[4]</property>			<factor>1.0</factor>			<offset-m>-0.3</offset-m>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>		</PropertyList>