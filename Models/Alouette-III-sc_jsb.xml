	<!--==============ROTOR   TAIL =============-->

        <animation>
            <type>select</type>
            <object-name>Queue-Disc_M</object-name>
            <condition>
		<and>
		    <not>
			<property>fdm/jsbsim/systems/crash-detect/crashed</property>
		    </not>
                    <not-equals>
                        <property>sim/current-view/view-number</property>
                        <value>0</value>
                    </not-equals>
		</and>
            </condition>
        </animation>

        <animation>
            <type>select</type>
            <object-name>Rotor-Pales-Queue</object-name>
            <condition>
		<and>
		    <property>fdm/jsbsim/systems/crash-detect/aero-on</property>
		    <less-than>
    			<!--<property>fdm/jsbsim/animation/rotor/tail/rpm</property>-->
			<property>sim/multiplay/generic/float[8]</property>
    			<value>1000</value>
		    </less-than>
                    <not-equals>
                        <property>sim/current-view/view-number</property>
                        <value>0</value>
                    </not-equals>
		</and>
            </condition>
        </animation>

    
