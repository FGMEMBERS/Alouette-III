        <model>
            <name>Rescue-Op</name>
            <path>Aircraft/Alouette-III/Models/Rescue/Rescue-Op.xml</path>
            <offsets>
                <x-m>-1.36017</x-m>
                <y-m>-1.03112</y-m>
                <z-m>-4.94186</z-m>
                <pitch-deg>0</pitch-deg>
            </offsets>
        </model>


        <model>
            <name>Rescue-Man</name>
            <path>Aircraft/Alouette-III/Models/Rescue/Rescue-Man.xml</path>
            <offsets>
                <x-m>-1.02689</x-m>
                <y-m>-0.844932</y-m>
                <z-m>-0.0</z-m>
                <pitch-deg>0</pitch-deg>
                <heading-deg>180</heading-deg>
            </offsets>
        </model>

        <animation>
            <type>rotate</type>
	    <object-name>Rescue-Op</object-name>
            <object-name>Treuil-Block</object-name>
            <object-name>Treuil-Bras</object-name>
            <object-name>Treuil-Crochet</object-name>
           <property>fdm/jsbsim/animation/mission-winch-pos-norm</property>
            <interpolation>
                <entry>
                    <ind>0.0</ind>
                    <dep>-5</dep>
                </entry>
                <entry>
                    <ind>1</ind>
                    <dep>10</dep>
                </entry>
            </interpolation>
            <center>
                <x-m>0.555458</x-m>
                <y-m>-0.328843</y-m>
                <z-m>0</z-m>
            </center>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>1</z>
            </axis>
        </animation>

        <animation>
            <type>select</type>
            <object-name>Rescue-Op</object-name>
            <object-name>Rescue-Man</object-name>
            <condition>
		<and>
                <equals>
                    <property>fdm/jsbsim/animation/left-cabin_door-pos-norm</property>
                    <value>1</value>
                </equals>
		<equals>
                    <property>fdm/jsbsim/animation/mission-winch-pos-norm</property>
                    <value>1</value>
                </equals>
		</and>
            </condition>
        </animation>

        <animation>
            <type>select</type>
            <object-name>Treuil-Crochet</object-name>
            <condition>
                <and>
                    <equals>
                        <property>/sim/model/rescue-lift</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
        </animation>
