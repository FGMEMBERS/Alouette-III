<!--interrupteur , marquages-->

<PropertyList>
    <condition>
        <and>
            <not>
                <or><property>sim/rendering/rembrandt</property> <property>sim/rendering/rembrandt/enabled</property> </or>
            </not>
        </and>
      </condition>
    <type>material</type>
    <emission>
            <factor-prop>fdm/jsbsim/electrical/lighting/instruments-norm</factor-prop>
            <red>0.60</red>
            <green>0.30</green>
            <blue>0.20</blue>
    </emission>
    <diffuse>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
    </diffuse>
    <ambient>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
    </ambient>
    <specular>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
    </specular>
    <shininess> 64</shininess>
</PropertyList>