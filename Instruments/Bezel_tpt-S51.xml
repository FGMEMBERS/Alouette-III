<PropertyList>

	<path>Models/bezel_tpt-s51.ac</path>
	<texture-path>Models/Textures</texture-path>

	<animation include="Instrument-Light5.xml">
		<object-name>Couronne</object-name>
	</animation>

        <model>
            <name>Light</name>
            <path>Aircraft/Alouette-III/Instruments/Light-Bezel_tpt-S51.xml</path>
            <offsets>
                <x-m>0</x-m>
                <y-m>0</y-m>
                <z-m>0</z-m>
                <pitch-deg>0</pitch-deg>
            </offsets>
        </model>

        <animation>
            <type>select</type>
            <object-name>Light</object-name>
            <condition>
                <or><property>sim/rendering/rembrandt</property> <property>sim/rendering/rembrandt/enabled</property> </or>
            </condition>
        </animation>





</PropertyList>